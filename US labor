CREATE TABLE users (
    id INTEGER PRIMARY KEY,
    name TEXT);
    
    


CREATE Table uslabor (
state TEXT,
population DECIMAL (19,4),
totalemployed DECIMAL (19,4),
partofunion DECIMAL (19,4));

